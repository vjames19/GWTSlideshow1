<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>GWTSlideshow.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>FileService</servlet-name>
		<servlet-class>edu.uprm.ece.hydroclimate.slideshow.server.FileServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FileService</servlet-name>
		<url-pattern>/edu.uprm.ece.hydroclimate.slideshow.GWTSlideshow/FileService</url-pattern>
	</servlet-mapping>

</web-app>
